// prettier-ignore
{
  "exclude": [
    "__tests__/**/*-test.ts"
  ],
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "components/*": [
        "src/components/*"
      ],
      "screens/*": [ "src/screens/*"],
      "navigation/*": [ "src/navigation/*"],
      "utils/*": [ "src/utils/*"],
      "constants/*": [ "src/constants/*"],
    }
  }
}
